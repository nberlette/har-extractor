{
  "name": "harx",
  "version": "1.0.0-beta.1",
  "description": "Simple CLI tool to extract HAR data.",
  "keywords": [
    "archieve",
    "directory",
    "extract",
    "file",
    "folder",
    "har",
    "unzip"
  ],
  "readme": "https://github.com/nberlette/harx#readme",
  "homepage": "https://n.berlette.com/harx",
  "bugs": "https://github.com/nberlette/harx/issues",
  "license": "MIT",
  "author": {
    "name": "Nicholas Berlette",
    "email": "nick@berlette.com",
    "url": "https://n.berlette.com/harx"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/harx.js",
  "types": "lib/harx.d.ts",
  "bin": {
    "harx": "./bin/cmd.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test",
    "bin": "bin"
  },
  "exports": {
    ".": {
      "import": "./lib/harx.js",
      "require": "./lib/harx.js",
      "default": "./lib/harx.js"
    },
    "./": "./src",
    "./types": "./lib/harx.d.ts",
    "./package.json": "./package.json"
  },
  "repository": "nberlette/harx",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 2,
    "semi": true,
    "useTabs": false,
    "arrowParens": "always",
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/har-format": "^1.2.5",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "glob": "^7.1.6",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-test-register": "^9.0.1",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "filenamify": "^2.1.0",
    "humanize-url": "^1.0.1",
    "make-dir": "^1.3.0",
    "meow": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  }
}
